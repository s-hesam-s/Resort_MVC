@model IEnumerable<Resort.Domain.Entities.Villa>

<h1>Villa List</h1>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>
                Name
            </th>
            <th>Price</th>
            <th>Sqft</th>
            <th>Occupancy</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>@obj.Name</td>
                <td>@obj.Price</td>
                <td>@obj.Sqft</td>
                <td>@obj.Occupancy</td>
            </tr>
        }
    </tbody>
</table>